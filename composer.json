{
    "name": "jazzman/wp-app",
    "require": {
        "php": ">=5.6",
        "jazzman/wp-app-config": "^1.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vasyl Sokolyk",
            "email": "vsokolyk@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JazzMan\\App\\":"src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "require-dev": {
        "roots/wordpress": "5.5.3"
    },
    "extra": {
        "installer-paths": {
            "vendor/wp/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
            "vendor/wp/plugins/{$name}/": ["type:wordpress-plugin"],
            "vendor/wp/themes/{$name}/": ["type:wordpress-theme"]
        },
        "wordpress-install-dir": "vendor/wp"
    }
}
